# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=your_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_auth_domain_here
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id_here
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_storage_bucket_here
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id_here
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=your_measurement_id_here

# Firebase Admin SDK
FIREBASE_PRIVATE_KEY=your_private_key_here
FIREBASE_CLIENT_EMAIL=your_client_email_here

# Google Cloud APIs
GOOGLE_CLOUD_PROJECT_ID=your_google_cloud_project_id
GOOGLE_CLOUD_PRIVATE_KEY=your_google_cloud_private_key
GOOGLE_CLOUD_CLIENT_EMAIL=your_google_cloud_client_email

# Google AI Platform APIs
GOOGLE_AI_API_KEY=your_google_ai_api_key_here
VEO_API_ENDPOINT=https://us-central1-aiplatform.googleapis.com/v1/projects/PROJECT_ID/locations/us-central1/publishers/google/models/veo-001:predict
IMAGEN_API_ENDPOINT=https://us-central1-aiplatform.googleapis.com/v1/projects/PROJECT_ID/locations/us-central1/publishers/google/models/imagen-3.0-generate-001:predict

# Replicate (primary LLM provider)
REPLICATE_API_TOKEN=your_replicate_api_token_here
# Examples: meta/llama-2-70b-chat, meta/llama-2-13b-chat, mistralai/mixtral-8x7b-instruct-v0.1
REPLICATE_MODEL=meta/llama-2-70b-chat

# OpenRouter (secondary fallback)
OPENROUTER_TOKEN=your_openrouter_token_here
# Examples: openai/gpt-4o-mini, anthropic/claude-3.5-sonnet, google/gemini-1.5-pro
OPENROUTER_MODEL=openai/gpt-4o-mini

# Hugging Face (tertiary fallback, optional)
# Preferred env var name is HF_TOKEN; HUGGINGFACE_TOKEN is also supported
HF_TOKEN=your_huggingface_token_here
HUGGINGFACE_TOKEN=
HF_PROVIDER= # e.g. sambanova | together | replicate (optional)
LLAMA_MODEL_ENDPOINT=https://api-inference.huggingface.co/models/meta-llama/Llama-4-Scout-17B-16E-Instruct

# NextJS
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000